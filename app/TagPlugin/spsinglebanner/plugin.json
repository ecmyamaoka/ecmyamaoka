{
  "PluginName": "SPバナー表示",
  "form": {
    "type": "object",
    "title": " ",
    "format": "grid",
    "properties": {
      "imgpath": {
        "title": "画像ファイルURL",
        "type": "string",
        "options": {
          "grid_columns": 12
        }
      },
      "linkurl": {
        "title": "リンクURL",
        "type": "string",
        "options": {
          "grid_columns": 12
        }
      },
      "bntype": {
        "type": "string",
        "title": "バナータイプ",
        "enum": [
          "通常",
          "SP フローティングバナー(下)"
        ],
        "options": {
          "grid_columns": 12
        }
      },
      "width": {
        "title": "幅(数字または%で入力)",
        "type": "string",
        "options": {
          "grid_columns": 6
        }
      },
      "height": {
        "title": "高さ(数字または%で入力)",
        "type": "string"
        ,
        "options": {
          "grid_columns": 6
        }
      },
      "starttime": {
        "title": "表示開始",
        "type": "string",
        "format": "datetime-local",
        "options": {
          "grid_columns": 6
        }
      },
      "endtime": {
        "title": "表示停止",
        "type": "string",
        "format": "datetime-local",
        "options": {
          "grid_columns": 6
        }
      },
      "bottom1": {
        "title": "ショップへ相談ボタン位置(px指定)",
        "type": "string",
        "options": {
          "grid_columns": 6
        }
      },
      "bottom2": {
        "title": "上へボタン位置(px指定)",
        "type": "string",
        "options": {
          "grid_columns": 6
        }
      },
      "bottom3": {
        "title": "クーポンボタン位置(px指定)",
        "type": "string",
        "options": {
          "grid_columns": 6
        }
      },
      "display": {
        "title": "表示",
        "type": "string",
        "enum": [
          "有効",
          "無効"
        ],
        "options": {
          "grid_columns": 6
        }
        
      }
    }
  },
  "sort": "11",
  "debug": "off",
  "preview": "on",
  "description": "スマートフォン用バナー表示ができます。",
  "id": "spsinglebanner",
  "icon": "data:image/png;base64,"
}